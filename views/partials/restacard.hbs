 {{#each data}}
   <div class="individual">

   <div class="imagecaja">
    <img src="{{this.[Foto1]}}" class="imageone">
    </div>
    <div class="infoCaja">
     <p class="nombre">{{this.Nombre}}</p>
    <p class="email">{{this.Email}}</p>
    <p class="direccion">{{this.Direccion}} </p>
    </div>
    <div class="formCaja">
    <form method="POST" action="/business-add/{{this.id}}"> 
    <button type="submit" class="btn btn-dark" > Realizar Compra</button> 
    </form>
    </div>
  </div> 



 {{/each }}

 